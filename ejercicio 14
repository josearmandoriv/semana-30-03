#include <stdio.h>
 int main(void){
 	int A,B,C; 
 	int d;
 	printf("Ejercicio 14 \n");
 	printf("indica el valor de A: \n");
 	scanf("%d", &A);
 	printf("indica el valor de B: \n");
 	scanf("%d", &B);
 	printf("indica el valor de C: \n");
 	scanf("%d", &C);
 	printf("indica la operaciÃ³n que quiere realizar\n 1) A>3 2) A>C 3) B!=C 4) A==3 5) A*B==15 6) C/B<A 7) C/B<A 8) A+B+C==5 9) (A+B==8) && (A-B==2) 10) (A+B==8)||(A-B==6) 11) (A>3) && (B>3) && (C>3) \n");
     scanf("%d",&d);
    switch(d)
	 {
	 	case 1:
	 	if (A>3)
	 printf("A es mayor que 3: %d\n",A);
	 else 
	 printf("A es menor que 3: %d\n",A);
	 break;

	case 2:
	if(A>C)
	printf("el valor de A es mayor que el de c: %d\n",A,C);
	else 
	printf("el valor de A es menor que el de C: %d\n",A,C);
	break;
	 
	 	case 3:
	 	if (B!= C )
	 	printf("el valor de B  es igual al de C: %d\n",B,C);
	 	else
	 	printf("el valor de B no es igual al de C: %d\n",B,C);
	 	break;
	
	 
	 	case 4:
	 	if(A==3)
	 	printf("el valor de A es igual a 3: %d\n",A);
	 	else 
	 	printf("el valor de A no es igual a 3: %d\n",A);
	 	break;
	 
	 
	 	case 5:
	 	if(A*B==15)
	 	printf("la multiplicacion de A*B es igual que 15: %d\n",A,B);
	 	else
	 	printf("la multiplicacion de A*B no es igual que 15: &d/n",A,B);
	 	break;
	 
			case 6:
			if(C/B<A)
			printf("la divicion de C/B es menor que A: %d\n",C,B,A);
			else 
			printf("la divicion de C/B es mayor que A: %d\n",C,B,A);
			break;
		 
		
			case 7:
			if (C/B==-10)
			printf("la divicion de C/B es igual que -10: %d\n",C,B);
			else 
			printf("la divicion de C/B no es igual que -10: %d\n",C,B);
			break;
		 
		
			case 8:
			if (A+B+C==5)
			printf("la suma de A+B+C es igual a 5: %d\n",A,B,C);
			else 
			printf("la suma de A+B+C no es igual a 5: %d\n",A,B,C);
			break;
			
		
		
			case 9:
			if((A+B == 8) && (A-B == 2))
			printf("A+B es igual a 8  y A-B es igual a 2: %d\n",A,B);
			else 
			printf("A+B no es igual a 8  y A-B no es igual a 2: %d\n",A,B);
			break;
		
		
			case 10:
			if ((A+B==8)||(A-B==6))
			printf("A+B es igual a 8 y A-B es igual a 6: %d\n",A,B);
			else 
			printf("A+B no es igual a 8 y A-B no es igual a 6: %d\n",A,B);
			break;
		
		
			case 11:
			if 	((A>3) && (B>3) && (C>3))
			printf("A es mayor que 3, B es mayor que 3 y C es mayor que 3: %d\n",A,B);
			else 
			printf("A no es mayor que 3, B no es mayor que 3 y C no es mayor que 3: %d\n",A,B);
			break; 
			
			default:
            printf("opcion invalida");
		}
		return 0;
	 }
